{
    "python-envs.defaultEnvManager": "ms-python.python:system",
    "python-envs.pythonProjects": [],
    "python.defaultInterpreterPath": "./intro-langchain-python/.venv/Scripts/python.exe",
    "python.terminal.activateEnvironment": true,
    "python.analysis.autoSearchPaths": true,
    "python.analysis.extraPaths": [
        "./intro-langchain-python/utils"
    ],
    "files.associations": {
        "*.md": "markdown"
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/node_modules": true
    },
    "editor.formatOnSave": true,
    "python.envFile": "${workspaceFolder}/intro-langchain-python/.env",
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    "editor.suggestOnTriggerCharacters": true
}